{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} {SourceContext} [{Level}] {Message}{NewLine}{Exception}",
          "Formatter": "Serilog.Formatting.Display.MessageTemplateTextFormatter, Serilog"
        }
      }
    ]
  }
}